{
    "name": "@attilagyongyosi/dayler",
    "version": "0.1.0",
    "description": "Personal day-management, calendar and task tracking platform.",
    "type": "module",
    "scripts": {
        "clean": "rimraf node_modules dist",
        "dev": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "test": "vitest run",
        "test:dev": "vitest",
        "lint": "concurrently --timings npm:lint:*",
        "lint:ts": "eslint --ext .ts,.tsx src/",
        "lint:paths": "path-linter",
        "metric:libyear": "libyear"
    },
    "author": {
        "email": "attilagyongyosi0@gmail.com",
        "name": "Attila Gyöngyösi",
        "url": "https://attilagyongyosi.com"
    },
    "dependencies": {
        "react": "18.2.0",
        "react-dom": "18.2.0"
    },
    "devDependencies": {
        "@attilagyongyosi/eslint-config": "0.4.0",
        "@attilagyongyosi/path-linter": "2.1.5",
        "@testing-library/jest-dom": "5.16.5",
        "@testing-library/react": "13.3.0",
        "@types/react": "18.0.17",
        "@types/react-dom": "18.0.6",
        "@typescript-eslint/parser": "5.38.0",
        "@typescript-eslint/eslint-plugin": "5.38.0",
        "@vitejs/plugin-react": "2.0.1",
        "@vitest/ui": "0.22.1",
        "concurrently": "7.4.0",
        "eslint": "8.23.1",
        "jsdom": "20.0.0",
        "libyear": "0.8.0",
        "rimraf": "3.0.2",
        "vite": "3.0.9",
        "vitest": "0.22.1"
    }
}
